<?xml version='1.0' encoding='utf-8'?>
<widget id="com.proxtopic.marcagto" version="1.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MarcaGTO</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <icon src="www/img/icon.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="moca_app_key" value="zhh32fxpRquqwh_jeXTkBQ" />
    <preference name="moca_app_secret" value="R1SHnh4wp0m3Oj5Yyz5j8QZ2x1Y=" />
    <engine name="android" spec="^6.3.0" />
    <engine name="browser" spec="^5.0.1" />
    <engine name="ios" spec="^4.5.4" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="com.innoquant.moca.phonegap" spec="https://github.com/mocaplatform/moca-phonegap-sdk.git#:/MOCASDK" />
    <plugin name="cordova-android-play-services-gradle-release" spec="^1.1.4">
        <variable name="PLAY_SERVICES_VERSION" value="11.+" />
    </plugin>
    <plugin name="cordova-plugin-android-permissions" spec="^1.0.0" />
    <plugin name="cordova-plugin-geolocation" spec="^3.0.0" />
    <plugin name="cordova-plugin-inappbrowser" spec="^2.0.1" />
    <plugin name="cordova.plugins.diagnostic" spec="^3.8.1" />
</widget>
